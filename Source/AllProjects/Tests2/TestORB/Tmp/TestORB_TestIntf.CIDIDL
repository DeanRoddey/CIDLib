    <CIDIDL:ClassIntf   CIDIDL:Name="TestOrbIntf"
                        CIDIDL:InterfaceId="1799C1BE4E47BA7E-19344A2250A462D2">

        <CIDIDL:Constants>
            <CIDIDL:Constant CIDIDL:Name="bVal" CIDIDL:Type="TBoolean" CIDIDL:Value="True">
                <CIDIDL:DocText>
                    This is the documentation for this constant.
                    It can have multiple lines of course, for formatting.
                        - And lines can be indented and that will be kept
                </CIDIDL:DocText>
            </CIDIDL:Constant>
            <CIDIDL:Constant CIDIDL:Name="c4Val" CIDIDL:Type="TCard4" CIDIDL:Value="139"/>
            <CIDIDL:Constant CIDIDL:Name="i2Val" CIDIDL:Type="TInt2" CIDIDL:Value="-39"/>
            <CIDIDL:Constant CIDIDL:Name="strVal" CIDIDL:Type="TString" CIDIDL:Value="This is a test"/>
        </CIDIDL:Constants>

        <CIDIDL:Types>
            <CIDIDL:Enum CIDIDL:Name="EDays" CIDIDL:Xlat="Inline">
                <CIDIDL:DocText>
                    This is the documentation for this enum.
                    It can have multiple lines of course, for formatting.
                        - And lines can be indented and that will be kept
                </CIDIDL:DocText>
                <CIDIDL:EnumVal CIDIDL:Name="EDay_Sunday" CIDIDL:Text="Sunday"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDay_Monday" CIDIDL:Text="Monday"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDay_Tuesday" CIDIDL:Text="Tuesday"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDay_Wednesday" CIDIDL:Text="Wednesday"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDay_Thursday" CIDIDL:Text="Thursday"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDay_Friday" CIDIDL:Text="Friday"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDay_Saturday" CIDIDL:Text="Saturday"/>
            </CIDIDL:Enum>

            <CIDIDL:Enum    CIDIDL:Name="EDirections" CIDIDL:Xlat="Inline">
