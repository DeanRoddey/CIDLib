                <CIDIDL:EnumVal CIDIDL:Name="EDir_Down" CIDIDL:Text="Down"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDir_Left" CIDIDL:Text="Left"/>
                <CIDIDL:EnumVal CIDIDL:Name="EDir_Right" CIDIDL:Text="Right"/>
            </CIDIDL:Enum>

            <CIDIDL:Enum CIDIDL:Name="EFlags" CIDIDL:Type="Bmp">
                <CIDIDL:EnumVal CIDIDL:Name="EFlag_TranBgn" CIDIDL:Value="0x0001"/>
                <CIDIDL:EnumVal CIDIDL:Name="EFlag_FlyoverEmph" CIDIDL:Value="0x0002"/>
                <CIDIDL:EnumVal CIDIDL:Name="EFlag_Border" CIDIDL:Value="0x0004"/>
